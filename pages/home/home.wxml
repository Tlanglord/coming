<!--pages/home/home.wxml-->
<swiper class='home-sw-cantianer' indicator-dots="{{indicatorDots}}" vertical="true"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image" />
    </swiper-item>
  </block>
</swiper>